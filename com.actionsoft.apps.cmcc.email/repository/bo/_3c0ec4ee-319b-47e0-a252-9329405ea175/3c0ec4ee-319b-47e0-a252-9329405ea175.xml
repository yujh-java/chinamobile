    <viewSql>SELECT '1' ID,'' PROCESSNAME,'' NOTEID,'' NOTENAME,'0' PARENTID,'' PROCESSTYPE,PROCESSAPP XSNAME,0 NOTENO FROM BO_ACT_PROCESS_DATA GROUP BY PROCESSAPP
UNION
SELECT PROCESSDEFINID ID,PROCESSNAME,'' NOTEID,'' NOTENAME,'1' PARENTID,PROCESSTYPE,PROCESSNAME XSNAME,0 NOTENO FROM 
BO_ACT_NOTEID_DATA WHERE ISMAIN='是' GROUP BY PROCESSDEFINID,PROCESSNAME,PROCESSTYPE
UNION
SELECT NOTEID ID,PROCESSNAME,NOTEID,NOTENAME,PROCESSDEFINID PARENTID,PROCESSTYPE,NOTENAME XSNAME,NOTENO  FROM BO_ACT_NOTEID_DATA WHERE ISMAIN='是' 
UNION
SELECT '2' ID,PROCESSNAME,NOTEID,NOTENAME,CREATEACTIVITYID PARENTID,PROCESSTYPE,NOTENAME XSNAME,NOTENO  FROM BO_ACT_NOTEID_DATA WHERE ISMAIN='否'
ORDER BY NOTENO</viewSql>
