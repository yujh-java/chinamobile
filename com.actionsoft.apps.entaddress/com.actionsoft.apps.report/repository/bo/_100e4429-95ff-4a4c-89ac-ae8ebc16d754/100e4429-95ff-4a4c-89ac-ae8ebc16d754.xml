    <title>流程查询中间视图（流程任务信息）</title>
    <name>VIEW_ACT_PROCESS_APP</name>
    <collection>true</collection>
    <system>false</system>
    <deleted>false</deleted>
    <categoryName>报表查询</categoryName>
    <administrator>admin  </administrator>
    <entityType>VIEW</entityType>
    <viewType>COMMON</viewType>
    <viewSql>SELECT ouser.username targetname,odept.departmentname targetdeptname,case when process.parentprocessinstid = '' then process.id else process.parentprocessinstid end processinstid,ctask.activitydefid,
ctask.target,ctask.targetdeptid FROM WFC_TASK ctask,WFC_PROCESS process,ORGUSER ouser,ORGDEPARTMENT odept 
where ctask.processinstid = process.id and process.isprocess=1 and ouser.userid = ctask.target and odept.id =ctask.targetdeptid  and (taskstate=1 or taskstate=11)
union 
SELECT null as targetname,null as targetdeptname, PROCESSINSTID,NULL AS ACTIVITYDEFID, NULL AS TARGET,
NULL AS TARGETDEPTID FROM WFH_TASK htask,WFC_PROCESS hprocess 
where htask.processinstid = hprocess.id and hprocess.isprocess=1 and hprocess.parentprocessinstid=''</viewSql>
