    <viewSql>SELECT ainfor.*,app.PROCESSAPP,app.PROCESSNAME,cprocess.PROCESSTITLE,cprocess.CREATEUSER, 
cprocess.CREATETIME, cprocess.CREATEUSERDEPTID,ouser.USERNAME TASKUSERNAME,
org.DEPARTMENTNAME TARGETDEPTNAME FROM VIEW_ACT_TASK_INFOR ainfor,BO_ACT_PROCESS_DATA app,
WFC_PROCESS cprocess,ORGUSER ouser,ORGDEPARTMENT org WHERE   app.PROCESSDEFNID=ainfor.PROCESSDEFID 
AND ainfor.PROCESSDEFID=cprocess.PROCESSDEFID and  ainfor.PROCESSINSTID = cprocess.ID 
AND ainfor.TARGET=ouser.USERID AND ainfor.TARGETDEPTID = org.ID</viewSql>
