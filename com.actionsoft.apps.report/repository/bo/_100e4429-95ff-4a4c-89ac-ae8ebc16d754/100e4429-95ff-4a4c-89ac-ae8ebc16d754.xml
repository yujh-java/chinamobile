    <categoryName>报表查询</categoryName>
    <administrator>admin  </administrator>
    <entityType>VIEW</entityType>
    <viewType>COMMON</viewType>
    <viewSql>SELECT ouser.USERNAME TARGETNAME,ode.DEPARTMENTNAME TARGETDEPTNAME,CASE WHEN pro.PARENTPROCESSINSTID = '' THEN pro.ID ELSE pro.PARENTPROCESSINSTID END PROCESSINSTID,ctask.ACTIVITYDEFID,
ctask.TARGET,ctask.TARGETDEPTID FROM WFC_TASK ctask,WFC_PROCESS pro,ORGUSER ouser,ORGDEPARTMENT ode 
where ctask.PROCESSINSTID = pro.ID and pro.ISPROCESS=1 AND ouser.USERID = ctask.TARGET AND ode.ID =ctask.TARGETDEPTID  AND (TASKSTATE=1 OR TASKSTATE=11)
UNION 
SELECT NULL AS TARGETNAME,NULL AS TARGETDEPTNAME, PROCESSINSTID,NULL AS ACTIVITYDEFID, NULL AS TARGET,
NULL AS TARGETDEPTID FROM WFH_TASK htask,WFC_PROCESS hprocess 
WHERE htask.PROCESSINSTID = hprocess.ID AND hprocess.ISPROCESS=1 and hprocess.PARENTPROCESSINSTID=''</viewSql>
